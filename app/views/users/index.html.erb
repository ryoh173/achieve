<div class="container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
        <%= profile_img(user) %>
        <%= link_to user.name, user %>
        <%= user.email %>
        <% if (current_user.id != user.id)  %>
          <%= render 'follow_form', user: user %>
        <% end %>
      </div>
  <% end %>
</div>
